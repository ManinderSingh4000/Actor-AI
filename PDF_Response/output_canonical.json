[
  {
    "scene_id": "scene_41bd8d21",
    "heading": {
      "raw": "INT. SMALL APARTMENT - NIGHT"
    },
    "blocks": [
      {
        "block_id": "dlg_7cdcf671",
        "order": 1,
        "type": "dialogue",
        "text": "Hello?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_d672496a",
        "order": 2,
        "type": "dialogue",
        "text": "Mary? Do you have any idea what time it is?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": {
          "raw": "O.S.",
          "emotions": []
        }
      },
      {
        "block_id": "dlg_351e159f",
        "order": 3,
        "type": "dialogue",
        "text": "I wouldn't be calling if it wasn't important.",
        "is_beat": false,
        "character": "MARY",
        "parenthetical": {
          "raw": "V.O.",
          "emotions": []
        }
      },
      {
        "block_id": "dlg_916190b1",
        "order": 4,
        "type": "dialogue",
        "text": "What happened?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_bfeec3c1",
        "order": 5,
        "type": "dialogue",
        "text": "They found me.",
        "is_beat": false,
        "character": "MARY",
        "parenthetical": {
          "raw": "V.O.",
          "emotions": []
        }
      },
      {
        "block_id": "dlg_1e8d7bab",
        "order": 6,
        "type": "dialogue",
        "text": "Who found you?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_3660ba5b",
        "order": 7,
        "type": "dialogue",
        "text": "I don't have time to explain. I messed up. I stayed too long.",
        "is_beat": false,
        "character": "MARY",
        "parenthetical": {
          "raw": "V.O.",
          "emotions": []
        }
      },
      {
        "block_id": "dlg_abdb4987",
        "order": 8,
        "type": "dialogue",
        "text": "I know. I know. But you're the only one I trust.",
        "is_beat": false,
        "character": "MARY",
        "parenthetical": {
          "raw": "V.O.",
          "emotions": []
        }
      },
      {
        "block_id": "dlg_451bf3f0",
        "order": 9,
        "type": "dialogue",
        "text": "Where are you right now?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_f978a30e",
        "order": 10,
        "type": "dialogue",
        "text": "I'm listening.",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_a30c5f30",
        "order": 11,
        "type": "dialogue",
        "text": "Near the river?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_96c0e1e5",
        "order": 12,
        "type": "dialogue",
        "text": "I'll be there.",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "blk_bd719dd5",
        "order": 13,
        "type": "action",
        "text": "A dim, cramped apartment lit by a single yellow bulb. Shadows cling to the walls. Rain taps steadily",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_f6b719dd",
        "order": 14,
        "type": "action",
        "text": "against the window, the sound sharp and relentless.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_4738812a",
        "order": 15,
        "type": "action",
        "text": "JOHN, early 30s, tense and tired, sits on a couch scrolling through his phone. The room is quiet--too",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_ae226e6b",
        "order": 16,
        "type": "action",
        "text": "quiet.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_14ead0eb",
        "order": 17,
        "type": "action",
        "text": "His phone suddenly RINGS.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_fdbc5b6d",
        "order": 18,
        "type": "action",
        "text": "John freezes. He hesitates, then answers.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_df8d7d33",
        "order": 19,
        "type": "action",
        "text": "A pause. Static crackles on the line.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_df2d669e",
        "order": 20,
        "type": "action",
        "text": "John... it's me.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_409ee252",
        "order": 21,
        "type": "action",
        "text": "John straightens immediately.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_04d8e16f",
        "order": 22,
        "type": "action",
        "text": "John rubs his face, already sensing something is wrong.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_6d88e09f",
        "order": 23,
        "type": "action",
        "text": "A breath. Then--",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_8c83b2dd",
        "order": 24,
        "type": "action",
        "text": "John stands up slowly.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_5f96e908",
        "order": 25,
        "type": "action",
        "text": "Silence. The rain grows louder.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_55f72085",
        "order": 26,
        "type": "action",
        "text": "John begins pacing.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_6bad0357",
        "order": 27,
        "type": "action",
        "text": "Mary, you can't just disappear for months and then call me like this.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_31c94443",
        "order": 28,
        "type": "action",
        "text": "John stops near the window, peering into the dark street below.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_6aefe1f1",
        "order": 29,
        "type": "action",
        "text": "Not safe. I can hear them moving outside.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_6ea8450e",
        "order": 30,
        "type": "action",
        "text": "John's jaw tightens.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_f14590b7",
        "order": 31,
        "type": "action",
        "text": "Then we don't have much time.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_aa90546a",
        "order": 32,
        "type": "action",
        "text": "A loud BANG is heard on Mary's end of the call.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_8b3aebc1",
        "order": 33,
        "type": "action",
        "text": "John... listen to me carefully.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_ec6691aa",
        "order": 34,
        "type": "action",
        "text": "Meet me at the old station. You remember the one.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_7812a009",
        "order": 35,
        "type": "action",
        "text": "John nods instinctively.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_85f3c347",
        "order": 36,
        "type": "action",
        "text": "Yes. Midnight. If I'm not there by twelve-thirty--",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_5eabe26f",
        "order": 37,
        "type": "action",
        "text": "She stops herself.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_b4a0254d",
        "order": 38,
        "type": "action",
        "text": "Don't say it.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_71595083",
        "order": 39,
        "type": "action",
        "text": "Another sound--FOOTSTEPS.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_15313c0e",
        "order": 40,
        "type": "action",
        "text": "Promise me you'll come.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_f3bfaf5d",
        "order": 41,
        "type": "action",
        "text": "The call abruptly CUTS OFF.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_4fa0267a",
        "order": 42,
        "type": "action",
        "text": "John stares at the phone. Then he grabs his jacket and keys.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      }
    ]
  },
  {
    "scene_id": "scene_f3aeb1d5",
    "heading": {
      "raw": "EXT. CITY STREET - NIGHT"
    },
    "blocks": [
      {
        "block_id": "dlg_7abbc7cb",
        "order": 1,
        "type": "dialogue",
        "text": "Where are you Mary ? Are you There ?",
        "is_beat": false,
        "character": "JOHN",
        "parenthetical": null
      },
      {
        "block_id": "dlg_faafe3e4",
        "order": 2,
        "type": "dialogue",
        "text": "I'm here , save me from this cruel Creature.",
        "is_beat": false,
        "character": "MARY",
        "parenthetical": {
          "raw": "V.O.",
          "emotions": []
        }
      },
      {
        "block_id": "blk_09aac94b",
        "order": 3,
        "type": "action",
        "text": "Neon lights flicker. Cars rush past, spraying water onto the pavement. The city feels alive, unaware.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_1e0e69ba",
        "order": 4,
        "type": "action",
        "text": "John steps onto the sidewalk, pulling his hood up. His eyes scan every passerby.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_5491957e",
        "order": 5,
        "type": "action",
        "text": "He moves quickly, blending into the crowd.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      }
    ]
  },
  {
    "scene_id": "scene_60b08375",
    "heading": {
      "raw": "INT. ABANDONED TRAIN STATION - NIGHT"
    },
    "blocks": [
      {
        "block_id": "blk_259af506",
        "order": 1,
        "type": "action",
        "text": "A massive, decaying structure. Broken clocks. Graffiti-covered walls. Wind howls through empty",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_4b5480d5",
        "order": 2,
        "type": "action",
        "text": "corridors.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_83830256",
        "order": 3,
        "type": "action",
        "text": "John enters cautiously.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_f3d9e765",
        "order": 4,
        "type": "action",
        "text": "He checks his watch:11:58 PM.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_9e96a47f",
        "order": 5,
        "type": "action",
        "text": "Footsteps echo in the distance.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_e2b9fb3d",
        "order": 6,
        "type": "action",
        "text": "John turns.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_a2b31e44",
        "order": 7,
        "type": "action",
        "text": "A figure emerges from the shadows.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_3140e813",
        "order": 8,
        "type": "action",
        "text": "It's Mary.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_236e27aa",
        "order": 9,
        "type": "action",
        "text": "Exhausted. Shaken. But alive.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_f11a82b9",
        "order": 10,
        "type": "action",
        "text": "Their eyes meet.",
        "is_beat": true,
        "character": null,
        "parenthetical": null
      },
      {
        "block_id": "blk_0c7f980d",
        "order": 11,
        "type": "action",
        "text": "The clock above them TICKS loudly as the screen FADES TO BLACK.",
        "is_beat": false,
        "character": null,
        "parenthetical": null
      }
    ]
  }
]